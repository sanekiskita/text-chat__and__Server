{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "parser": "babel-eslint",
    "rules": {
        "jsx-quotes": [
            1,
            "prefer-single"
          ],
          "ignorePattern":true
    },
    "plugins": [
        "prettier"
      ]
}
